
@{
    ViewData["Title"] = "List";
}
@model List<QuestionModel>

<h1>All questions:</h1>
<br /><br />
@*@if(Model.Count==0)
{
    <a>Currently no questions on the page!</a>
}*@
@foreach (var question in Model)
{
<li>
    @question.Title
    <form action="QuestionDetails">
        <button name="details" type="submit"> See answers</button>
    </form>

    <form action="Privacy">
        <button  type="submit">Edit</button>
    </form>

    <form>
        <button type="submit">Delete</button>
    </form>
</li>
}
    
